  "engines": {
    "node": ">=10"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "index.d.ts",
  "repository": "https://github.com/sluukkonen/soles",
  "author": "Sakumatti Luukkonen <sakumatti.luukkonen@gmail.com>",
  "license": "MIT",
  "private": false,
  "sideEffects": false,
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@babel/preset-env": "^7.13.10",
    "@rollup/plugin-replace": "^2.4.1",
    "@types/benchmark": "^2.1.0",
    "@types/jest": "^26.0.20",
    "babel-jest": "^26.6.3",
    "bundlewatch": "^0.3.2",
    "eslint": "^7.21.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.2.0",
    "eslint-plugin-prettier": "^3.3.1",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "rollup": "^2.41.0",
    "rollup-plugin-terser": "^7.0.2",
    "terser": "^5.6.0",
    "tsd": "^0.14.0",
    "typescript": "^4.2.3",
    "yargs": "^16.2.0",
    "yarn": "^1.22.10"
  },
  "scripts": {
    "benchmark": "yarn build && NODE_ENV=production node benchmark",
    "build": "rm -rf dist && node generate-index.js && rollup -c",
